    <div class="form-group">
        <label asp-for="PrecioUsd" class="control-label"></label>
        <input asp-for="PrecioUsd" class="form-control" />
        <span asp-validation-for="PrecioUsd" class="text-danger"></span>
    </div>
    -->
</form>
